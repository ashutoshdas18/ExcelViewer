<head>
    <style>
        @import url('https://fonts.googleapis.com/css2?family=Poppins:wght@300;400;500&display=swap');
        .headerContainer{
            height: 100px;
            max-width: 1340px;
            margin-left: auto;
            margin-right: auto;
            display: flex;
            align-items: center;
            font-family: 'Poppins',sans-serif;
        }
        .logo{
            display: flex;
            height: 60px;
            align-items: center;
            padding-left: 20px;
        }
        .logo>h2{
            font-family: 'Poppins', sans-serif;
            font-weight: 500;
            color: white;
        }
        .logo>img{
            height: 45px;
            width: 45px;
            padding-right: 15px;
        }
        .others{
            width: 85%;
            display: flex;
            justify-content: space-evenly;
        }
        .centre{
            display: flex;
            align-items: center;
            justify-content: space-evenly;
            width: 50%;
        }
        .centre>div{
            color: white;
        }
        .end{
            display: flex;
        }
        .end>div{
            padding: 18px 50px;
            border: 1px solid white;
            background: none;
            color: white;
            border-radius: 5px;
        }
    </style>
</head>
<body>
    <div class="headerContainer">
    <div class="logo">
        <img src="./xls.png" alt="" class="logoImg">
        <h2>ExcelViewer</h2>
    </div>
    <div class="others">
        <div class="centre">
            <div class="email">Services</div>
            <div class="contact">Contacts</div>
            <div class="faq">FAQ</div>
        </div>
        <div class="end">
            <div class="login">Login / Join</div>
        </div>
    </div>
    
</div>
</body>